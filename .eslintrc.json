{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"func-names": 0,
		"space-before-function-paren": 0,
		"indent": [
			"error",
			2
		],
		"no-plusplus": 0,
    "max-len": [
      "error",
      {
        "code": 150
      }
    ]
	}
}